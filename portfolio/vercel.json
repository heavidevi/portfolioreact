{
  "version": 2,
  "builds": [
    {
      "src": "portfolio/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "portfolio/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/portfolio/index.html"
    }
  ],
  "buildCommand": "cd portfolio && npm run build",
  "outputDirectory": "portfolio/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/portfolio/index.html"
    }
  ]
}